#Calvin Progress

import xlrd, 

book = xlrd.open_workbook('phailin_tweets.xls')
worksheet = book.sheet_by_name('Sheet1')
num_rows = worksheet.nrows - 1
curr_row = -1
while curr_row < num_rows: 
	curr_row += 1
	row  = worksheet.row(curr_row)
	print row
		
	


#wrote script using xlrd to read a excel file.

import os

os.listdir('.')

