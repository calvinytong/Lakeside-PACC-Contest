import csv
from datetime import datetime

hourly = {}

with open('phailin_tweets.csv', 'rU') as file:
        reader = csv.reader(file)
        headers = next(reader, None)
        for data in reader:
                current = datetime.strptime(data[2], '%m/%d/%y %H:%M')
                byhour = datetime.strftime(current, '%y/%m/%m %H')
                if byhour in hourly:
                        hourly[byhour] += 1
                else:
                        hourly[byhour] = 1

